{
  "extends": "./tsconfig.json",
  "angularCompilerOptions:": {
    "enableIvy": true
  },
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "./node_modules/network-information-types"
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
